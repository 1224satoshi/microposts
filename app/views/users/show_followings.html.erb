<div class="row">
      <aside class="col-md-4">
    　　<section class="user_info">
            <h1>
                <%= gravatar_for current_user %>
                <%= current_user.name %>
            </h1>
        </section>
      </aside> 
      <div class="users">
         <h3>followings</h3>
           <ul>
          　　<% @users.each do |user| %>
            　　<li>
                    <%= link_to user.name, user %>
                    <%= gravatar_for user %>
            　　</li>
          　　<% end %>
    　　　</ul>
     </div>
</div>